{
  "name": "aiengines-api",
  "version": "0.1.0",
  "description": "API server for AI Engines Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "security:audit": "npm audit --audit-level=moderate",
    "docker:build": "docker build -t aiengines-api .",
    "docker:run": "docker run -p 3001:3001 aiengines-api",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "health": "curl -f http://localhost:3001/health || exit 1"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.13.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/websocket": "^8.3.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "zod": "^3.22.4",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "prettier": "^3.1.1",
    "@types/redis": "^4.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "api",
    "fastify",
    "typescript",
    "ai",
    "agents",
    "codex"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aiengines-api.git"
  },
  "license": "MIT",
  "author": "AI Engines Team"
}